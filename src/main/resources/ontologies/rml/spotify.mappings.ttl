@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix domain: <http://www.csd.auth.gr/spotify#>.

@base <http://www.csd.auth.gr/spotify/>.

<#ArtistDataPropertiesMapping1> a rr:TriplesMap;
                 rml:logicalSource [
                     rml:source "../../datasets/updated/artists_cleaned.csv" ;
                     rml:referenceFormulation ql:CSV
                   ];

                 rr:subjectMap [
                     rr:template "http://www.csd.auth.gr/spotify/data/artists/{Artist_id}";
                     rr:class domain:SpotifyArtist
                   ];

                 rr:predicateObjectMap [
                     rr:predicate domain:artistName;
                     rr:objectMap [
                                    rml:reference "Artist";
                                    rr:datatype xsd:string
                                  ]
                   ];

                 rr:predicateObjectMap [
                     rr:predicate domain:dailyStreams;
                     rr:objectMap [
                                    rml:reference "Daily";
                                    rr:datatype xsd:float
                                  ]
                   ];

                 rr:predicateObjectMap [
                     rr:predicate domain:featureStreams;
                     rr:objectMap [
                                    rml:reference "As feature";
                                    rr:datatype xsd:float
                                  ]
                   ];

                 rr:predicateObjectMap [
                     rr:predicate domain:leadStreams;
                     rr:objectMap [
                                    rml:reference "As lead";
                                    rr:datatype xsd:float
                                  ]
                   ];

                 rr:predicateObjectMap [
                     rr:predicate domain:soloStreams;
                     rr:objectMap [
                                    rml:reference "Solo";
                                    rr:datatype xsd:float
                                  ]
                   ];

                 rr:predicateObjectMap [
                     rr:predicate domain:totalStreams;
                     rr:objectMap [
                                    rml:reference "Streams";
                                    rr:datatype xsd:float
                                  ]
                   ].

